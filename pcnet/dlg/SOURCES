MAJORCOMP = net
MINORCOMP = AMDDLG

TARGETNAME=AMDDLG
TARGETTYPE=DYNLINK
TARGETPATH=$(BASEDIR)\lib
DLLBASE=0x07800000

INCLUDES=.

MSC_WARNING_LEVEL=/W3 /WX

SOURCES=amddlg.rc     \
	amddlg.c

RELATIVE_DEPTH=..\..


